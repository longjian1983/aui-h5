<ui>
    <aui-page transition="slide"></aui-page>
</ui>


<script>
import router from '../routers';


export default class Frame{
    
    static get tag(){
        return 'frame';
    }

    created(){
        const hash = (location.hash||'/').replace('#', '');
        router.go(hash);
    }
}

</script>

